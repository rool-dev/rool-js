{
  "name": "@rool-dev/svelte",
  "version": "0.1.8",
  "description": "Svelte 5 runes for Rool Spaces",
  "packageManager": "pnpm@10.17.1",
  "type": "module",
  "svelte": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "svelte-package -i src -o dist",
    "dev": "svelte-package -i src -o dist --watch",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json",
    "clean": "rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://docs.rool.dev/svelte",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rool-dev/rool-js.git",
    "directory": "packages/svelte"
  },
  "keywords": [
    "rool",
    "svelte",
    "svelte5",
    "runes",
    "reactive",
    "real-time"
  ],
  "author": {
    "name": "Rool Limited",
    "email": "info@rool.dev",
    "url": "https://rool.dev"
  },
  "license": "MIT",
  "dependencies": {
    "@rool-dev/sdk": "workspace:*"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.9.3"
  }
}
