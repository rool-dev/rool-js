{
  "name": "rool-js",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Rool JavaScript SDK and CLI",
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "build": "pnpm -r build",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean",
    "docs:prebuild": "pnpm --filter @rool-dev/cli generate-readme && node docs/build-docs.js",
    "docs:dev": "pnpm run docs:prebuild && astro dev --root docs",
    "docs:build": "pnpm run docs:prebuild && astro build --root docs",
    "docs:preview": "astro preview --root docs"
  },
  "devDependencies": {
    "astro": "^5.2.5",
    "@astrojs/starlight": "^0.32.2",
    "sharp": "^0.33.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rool-dev/rool-js.git"
  },
  "author": {
    "name": "Lightpost One",
    "email": "info@lightpost.one",
    "url": "https://lightpost.one"
  },
  "license": "MIT"
}
